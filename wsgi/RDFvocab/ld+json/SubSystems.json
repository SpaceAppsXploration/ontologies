{
  "@context": {
    "@base": "http://ontology.projectchronos.eu/subsystems",
    "spacecraft": "http://ontology.projectchronos.eu/spacecraft/",
    "subsystem": "http://ontology.projectchronos.eu/subsystems/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "dbpedia": "http://dbpedia.org/ontology/",
    "xml": "http://www.w3.org/2001/XMLSchema#"
  },
  "@id": "",
  "@type": "http://www.w3.org/2002/07/owl#Ontology",
  "rdfs:label": "Subsystems that run tasks that are needed for the spacecraft to be working and fulfilling the objectives of the mission. Words separated by _ has to be read as separated concepts, camelCase in they are the same concepts (see umbel.org for this norm)",
  "rdfs:comment": "These ontology contains classes of different subsystems present in a spacecraft.",
  "defines": [
         {
           "@id" : "http://ontology.projectchronos.eu/subsystems/manufacturer",
           "@type" : [
                { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
           ],
           "rdfs:label": "manufacturer"
         },
         {
            "@id" : "http://ontology.projectchronos.eu/subsystems/function",
            "@type" : [
                { "@id": "http://www.w3.org/2002/07/owl#AsymmetricProperty" },
                { "@id": "http://www.w3.org/2002/07/owl#IrreflexiveProperty" },
                { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
            ],
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:comment" : "The function and the objective what the device performs or make possible",
            "rdf:label" : "what the device is used for",
            "rdfs:label" : "function",
            "skos:altLabel" : "is used for"
        },
        {
            "@id" : "http://ontology.projectchronos.eu/subsystems/unitOfMeasure",
            "@type" : [
                { "@id": "http://www.w3.org/2002/07/owl#AsymmetricProperty" },
                { "@id": "http://www.w3.org/2002/07/owl#IrreflexiveProperty" },
                { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
            ],
            "owl:sameAs": [
                { "@id": "http://umbel.org/umbel/rc/UnitOfMeasure"}, 
                { "@id": "http://dbpedia.org/resource/Units_of_measurement" }
            ],
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral" },
            "rdfs:comment" : "Property to set the unit of measure used by a physical value",
            "rdfs:label" : "unitOfMeasure"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/subSystemType",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "skos:prefLabel": "A property that references the subsystem to the kind of the devices it holds.",
            "rdfs:comment": "Every subsystem contains an homogeneous group of devices.",
            "rdfs:label": "subSystemType",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/isStandard",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "skos:prefLabel": "A property that references the standard platform for which the subsystem has been designed.",
            "rdfs:comment": "There are many different standards that imply quantitative and qualitative differences",
            "rdfs:label": "isStandard",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasVolume",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Volume" }, 
                { "@id": "http://dbpedia.org/resource/Volume" }
            ],
            "rdfs:label": "hasVolume",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMinAmpere",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Ampere" }, 
                { "@id": "http://dbpedia.org/resource/Ampere" }
            ],
            "rdfs:label": "hasMinAmpere",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMaxAmpere",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Ampere" }, 
                { "@id": "http://dbpedia.org/resource/Ampere" }
            ],
            "rdfs:label": "hasMaxAmpere",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMass",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Mass" }, 
                { "@id": "http://dbpedia.org/resource/Mass" }, 
                { "@id": "http://schema.org/Mass" }
            ],
            "rdfs:label": "hasMass",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasPower",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://umbel.org/umbel/rc/Power" }, 
                { "@id": "http://dbpedia.org/resource/Power_(physics)"}
            ],
            "rdfs:label": "hasPower",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasSpecificImpulse",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": { "@id": "http://dbpedia.org/resource/Specific_impulse" },
            "rdfs:label": "hasSpecificImpulse",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasMonetaryValue",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://www.w3.org/2001/XMLSchema#float" },
            "skos:exactMatch": [
                { "@id": "http://dbpedia.org/resource/Price" }, 
                { "@id": "http://umbel.org/umbel/rc/MonetaryValue" }, 
                { "@id": "http://schema.org/PriceSpecification" }
            ],
            "rdfs:label": "hasMonetaryValue",
            "rdfs:comment": "Amount of money it can be bought for, or an esteem of value.",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:label": "hasWireInWith",
            "rdfs:comment": "This device receive input from another device",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdf:range" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
            "rdfs:label": "hasWireOutWith",
            "rdfs:comment": "This device send output to another device",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/typeOfPropellant",
            "rdf:domain" : { "@id": "http://ontology.projectchronos.eu/spacecraft/Spacecraft_Propulsion" },
            "rdfs:label": "typeOfPropellant",
            "rdfs:comment": "Type of fueling used by a spacecraft rocket engine",
            "@type" : { "@id": "http://www.w3.org/2002/07/owl#ObjectProperty" }
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Detector",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://umbel.org/umbel/rc/Sensor_Device" },
                { "@id": "http://ontology.projectchronos.eu/spacecraft/SubSystems_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/objective",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/isComponentOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Payload_Spacecraft"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
                    "owl:hasValue" : [
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Communication" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_CDH" }
                    ]
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasMinAmpere",
                    "owl:minCardinality" : 1
                },
                              {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasMaxAmpere",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasVoltage",
                    "owl:minCardinality" : 1
                }
            ],
            "rdfs:label": "Spacecraft_Detector",
            "rdfs:comment": "A space detector is a sensor supported by another device that let it collect data, that is deployed into a spacecraft and works outside Earth lower atmosphere",
            "skos:prefLabel": "Any scientific instrument carried by a space probe or an artificial satellite",
            "spacecraft:isComponentOf": "http://ontology.projectchronos.eu/spacecraft/Payload_Spacecraft"
        },
