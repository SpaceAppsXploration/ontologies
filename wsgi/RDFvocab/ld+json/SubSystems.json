            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/function",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/ProjectilePropelling"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/spacecraft/isSubsystemOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Spacecraft"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/typeOfPropellant",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasSpecificImpulse",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/RocketEngine"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
                    "owl:hasValue" : [
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_CDH" }
                    ]
                }
            ],

            "skos:prefLabel": "The set of subsystems needed to make a spacecraft moving in space",
            "rdfs:label": "Spacecraft_Propulsion.",
            "rdfs:comment": "Complex devices-subsystems used for impelling (processes of applying a force which results in translational motion) a spacecraft, in the specific http://umbel.org/umbel/rc/ProjectilePropelling",
            "spacecraft:isComponentOf": "http://ontology.projectchronos.eu/spacecraft/Spacecraft"

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/function",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/ElectricalPowerGeneration"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/spacecraft/isSubsystemOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Spacecraft"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasEfficiency",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasVoltage",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/ElectricalDevice"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
                    "owl:hasValue" : [
                      { "@id":"http://ontology.projectchronos.eu/subsystems/Spacecraft_Communication" },
                      { "@id":"http://ontology.projectchronos.eu/subsystems/Spacecraft_CDH" },
                      { "@id":"http://ontology.projectchronos.eu/subsystems/Spacecraft_AODCS" },
                      { "@id":"http://ontology.projectchronos.eu/subsystems/Spacecraft_Propulsion" },
                      { "@id":"http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal" }
                    ]
                }
            ],

            "skos:prefLabel": "The set of subsystems needed to make a spacecraft to collect energy to operate",
            "rdfs:label": "Spacecraft_PrimaryPower.",
            "rdfs:comment": "Complex devices-subsystems used for collecting energy.",
            "spacecraft:isComponentOf": "http://ontology.projectchronos.eu/spacecraft/Spacecraft"
        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_BackupPower",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/function",
                    "owl:hasValue" : "http://dbpedia.org/resource/rc/Energy_storage"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/spacecraft/isSubsystemOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Spacecraft"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/ElectricalDevice"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
                    "owl:hasValue" : [
                        { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower" }
                    ]
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireOutWith",
                    "owl:hasValue" : [
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_CDH" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Propulsion" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal" },
                      { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Communication" }
                    ]
                }
            ],

            "rdfs:label": "Spacecraft_BackupPower",
            "skos:prefLabel": "The set of subsystems needed to make a spacecraft to store energy from the primary power source.",
            "rdfs:comment": "Complex devices-subsystems used for storing energy.",
            "spacecraft:isComponentOf": "http://ontology.projectchronos.eu/spacecraft/Spacecraft"

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/function",
                    "owl:hasValue" : "http://dbpedia.org/resource/Process_control"
                }
            ],

            "rdfs:label": "Spacecraft_Thermal",
            "skos:prefLabel": "Artifacts or devices used to maintain the temperature of spacecraft's subsystems and payloads into a given range, to permit nominal and survival mode for all the duration of the mission .",
            "rdfs:comment": "Shields, shells or any device insulation from/reflecting radiation exploiting emission and absorption events"

        },

        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal_PassiveDevice",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/PhysicalDevice"
                }
            ],

            "rdfs:label": "Spacecraft_Thermal_PassiveDevice",
            "skos:prefLabel": "Device of of class Thermal that does NOT consume energy from the spacecraft power source.",
            "rdfs:comment": "They are passive because they mostly transform radiation into heating/cooling ",
            "spacecraft:isComponentOf": "http://ontology.projectchronos.eu/spacecraft/Spacecraft"

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Thermal_ActiveDevice",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Spacecraft_Thermal" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/PoweredDevice"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/hasWireInWith",
                    "owl:hasValue" : [
                        { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_PrimaryPower" },
                        { "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_BackupPower" }
                    ]
                }
            ],

            "rdfs:label": "Spacecraft_Thermal_ActiveDevice",
            "skos:prefLabel": "Device that of class Thermal that consumes energy from the spacecraft power source.",
            "rdfs:comment": "Complex devices-subsystems used to protect sensors or electronic devices from over/under-heating, like refrigeration absorption."

        },
        {
            "@id": "http://ontology.projectchronos.eu/subsystems/Spacecraft_Structure",
            "@type" : "http://www.w3.org/2002/07/owl#Class",
            "rdfs:subClassOf" : [
                { "@id": "http://ontology.projectchronos.eu/spacecraft/Subsystem_Spacecraft" },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsytems/function",
                    "owl:hasValue" : "http://dbpedia.org/resource/Structural_system"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/spacecraft/isSubsystemOf",
                    "owl:hasValue" : "http://ontology.projectchronos.eu/spacecraft/Spacecraft"
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/standsMinTemperature",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/standsMaxTemperature",
                    "owl:minCardinality" : 1
                },
                {
                    "@type" : "http://www.w3.org/2002/07/owl#Restriction",
                    "owl:onProperty" : "http://ontology.projectchronos.eu/subsystems/subSystemType",
                    "owl:hasValue" : "http://umbel.org/umbel/rc/PhysicalDevice"
